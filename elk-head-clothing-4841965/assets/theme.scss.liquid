	.fancybox-prev span,
	.fancybox-next span {
	  background-image: url('fancybox_sprite.png');
	}
	#fancybox-loading {
	  position: fixed;
	  top: 50%;
	  left: 50%;
	  margin-top: -22px;
	  margin-left: -22px;
	  background-position: 0 -108px;
	  opacity: 0.8;
	  cursor: pointer;
	  z-index: 8060;
	}
